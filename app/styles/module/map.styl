.m-map {
    z-index: 10;
    position: absolute;
    width: 100%;
    height: 100%;
    padding-top: 120px;
    padding-bottom: 100px;

    @media $medium {
        padding-top: 0;
        padding-bottom: 0;
    }

    .leaflet-container {
        background: white;
        width: 100%;
        height: 100%;
    }

    .leaflet-marker-icon {
        opacity: 0.9;
    }
}
